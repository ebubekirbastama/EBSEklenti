{
  "manifest_version": 2,
  "name": "EBS Url Açıcı",
  "version": "1.0",
  "description": "A sample extension that demonstrates how to interact with the DOM.",
  "permissions": [
  "activeTab",
  "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
